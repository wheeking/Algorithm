import java.util.Arrays;
import java.util.Scanner;

public class Solution {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int T = sc.nextInt();
		
		for (int test_case = 1; test_case <= T; test_case++) {
			int N = sc.nextInt();
			int K = sc.nextInt();
			double[] score = new double[N+1];
			
			for (int i = 1; i <= N; i++) {
				double mid = sc.nextInt();
				double fin = sc.nextInt();
				double hw = sc.nextInt();
				double hap = mid*35/100 + fin*45/100 + hw*20/100;
				
				score[i] = hap;
			}
			
			double temp = score[K];
			Arrays.sort(score);		// 0부터 N-1
			int rank = 0;;
			for (int i = 0; i < score.length; i++) {
				if (score[i] == temp)
					rank = N - i + 1;;
			}
			double tt = (double)N/10;
			double ans = Math.ceil(rank/tt);
			
			int Aans = (int)ans;
			String str = "";
			
			switch (Aans) {
			case 1:
				str = "A+";
				break;
			case 2:
				str = "A0";
				break;
			case 3:
				str = "A-";
				break;
			case 4:
				str = "B+";
				break;
			case 5:
				str = "B0";
				break;
			case 6:
				str = "B-";
				break;
			case 7:
				str = "C+";
				break;
			case 8:
				str = "C0";
				break;
			case 9:
				str = "C-";
				break;
			case 10:
				str = "D0";
				break;
			}
			
			
			System.out.println("#" + test_case + " " + str);
		}
	}
}

