import java.util.Scanner;

public class Solution {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int T = sc.nextInt();
		
		for (int test_case = 1; test_case <= T; test_case++) {
			int a = sc.nextInt();
			int b = sc.nextInt();
			
			String ans;
			if (a > b) {
				ans = ">";
			} else if (a == b) {
				ans = "=";
			} else
				ans = "<";
			
			System.out.println("#" + test_case + " " + ans);
		}
	}
}
